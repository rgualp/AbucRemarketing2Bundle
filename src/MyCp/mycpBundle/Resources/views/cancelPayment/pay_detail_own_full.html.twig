<table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>Fecha del pago</th>
        <th>Tipo de Cancelación</th>
        <th>Código de la casa,Nombre y Correo</th>
        <th>Monto del Pagar</th>
        <th>Estado del pago</th>
        <th>Usuario</th>
        <th>Fecha</th>

    </tr>
    </thead>
    <tbody>
    {% for pay in pays %}
        <td>{{ pay.getPaymentDate | date("d/m/Y") }}</td>
        <td>{{ pay.getCancelId.getType.getCancelName }}</td>.
        <td><b>Código de Casa:</b> {{ pay.getUserCasa.getOwnMcpCode }} - <b>Nombre de la casa:</b> {{ pay.getUserCasa.getOwnName }}- <b>Nombre del propietario:</b> {{ pay.getUserCasa.getOwnHomeowner1 }}- <b>Correo del propietario:</b> {{ pay.getUserCasa.getOwnEmail1 }}</td>
        <td>{{ pay.getPayAmount }} CUC </td>
        <td>{{ pay.getType.translations[0].nomLangDescription }}</td>
        <td>{{ pay.getUser.getUserCompleteName }}</td>
        <td>{{pay.registerDate | date ("d/m/Y")}}</td>
    {% endfor %}
    </tbody>
</table>
