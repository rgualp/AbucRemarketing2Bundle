<ul class="nav nav-tabs">
    <li class="active">
        <a href="#reservas" data-toggle="tab">Agencia</a>
    </li>
</ul>
<table class="table  table-striped" style="font-size: 12px;">
    <tr>
        {#<td rowspan="3"  style="width: 100px"><a &#123;&#35;title="Editar" href="{{ path("mycp_edit_user_tourist", {'id_user': user.userId}) }}"&#35;&#125;>{{ render(controller('mycpBundle:BackendUser:getUserPhotoPath', {'userId': user.userId})) }}</a></td>#}
        <td><b>Agencia:</b> <a {#title="Editar" href="{{ path("mycp_edit_user_tourist", {'id_user': user.userId}) }}"#}>{{ user.getUserLastName }}</a></td>
        <td><b>Correo:</b> <a href="mailto:{{ user.getUserEmail | lower }}">{{ user.getUserEmail | lower }}</a></td>
        <td colspan="2"><b>País:</b> {{ user.getUserCountry }}
            {% if user.getUserCity != null %}(Ciudad: {{ user.getUserCity }}){% endif %}
        </td>
    </tr>
    <tr>
        <td colspan="3"><b>Dirección:</b> {% if user.getUserAddress != null %}{{ user.getUserAddress }}{% else %}-{% endif %}</td>
        <td><b>Teléfono:</b> {{ user.getUserPhone }}</td>
    </tr>
    {% if clientOfAg is defined and clientOfAg != null %}
        <tr>
            <td colspan="4"><b>Cliente: </b>{{ clientOfAg.getFullName }}</td>
        </tr>


    {% endif %}
</table>