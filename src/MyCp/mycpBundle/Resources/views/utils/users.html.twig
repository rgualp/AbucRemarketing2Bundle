{% if users %}
<option value=""></option>
{% for user in users %}
    <option class="{{user.getUserSubrole.getRoleName}}" {% if( selected == user.getUserId) %} selected="TRUE" {% endif %} value="{{ user.getUserId }}"> {% if user.getUserName %}{{ user.getUserName }}{% else %}{{ user.getUserName }}{% endif %}</option>
{% endfor %}
{% endif %}