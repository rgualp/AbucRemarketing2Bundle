<?php

namespace MyCp\mycpBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * destinos
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="MyCp\mycpBundle\Entity\destinosRepository")
 */
class destinos
{
    /**
     * @var integer
     *
     * @ORM\Column(name="des_id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $des_id;

    /**
     * @var string
     *
     * @ORM\Column(name="des_name", type="string", length=255)
     */
    private $des_name;

    /**
     * @var integer
     *
     * @ORM\Column(name="des_order", type="integer")
     */
    private $des_order;

    /**
     * @var boolean
     *
     * @ORM\Column(name="des_active", type="boolean")
     */
    private $des_active;
}