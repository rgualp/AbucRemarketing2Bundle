<?php

namespace MyCp\mycpBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * langphoto
 *
 * @ORM\Table(name="langphoto")
 * @ORM\Entity(repositoryClass="MyCp\mycpBundle\Entity\langPhotoRepository")
 */
class langPhoto
{
    /**
     * @var integer
     *
     * @ORM\Column(name="lang_pho_id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $lang_pho_id;

    /**
     * @ORM\ManyToOne(targetEntity="photo",inversedBy="")
     * @ORM\JoinColumn(name="lang_pho_photo",referencedColumnName="pho_id")
     */
    private $lang_pho_photo;

    /**
     * @ORM\ManyToOne(targetEntity="lang",inversedBy="")
     * @ORM\JoinColumn(name="lang_pho_alb_id",referencedColumnName="lang_id")
     */
    private $lang_pho_alb_id;
}