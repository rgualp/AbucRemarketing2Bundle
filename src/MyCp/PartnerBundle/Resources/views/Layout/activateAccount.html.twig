{% extends 'LayoutBundle:Layout:layout_partner_frontend.html.twig' %}

{% block carousel %}
{% endblock %}
{% block navbar %}
    {%  include 'PartnerBundle:Layout:navbar.html.twig' with { 'navbar_bg_class' : true }  %}
{% endblock %}
{% block login_register %}
    {% include 'LayoutBundle:Security:overlay_login.html.twig' %}
{% endblock %}
{% block body %}
    <div class="space space-70"></div>
    <div class="container m-t-xl text-center">
        <div class="row text-left" style="margin-left: 11%">
            <img alt="image" class="img-responsive" src="{{ asset('bundles/partner/img/icon/logo_color_'~app.session.get('app_lang_code')|lower)}}.png">
        </div>
        <div class="row" style="margin-top: 7%; line-height: 65px;">
            <div class="col-md-12">
                <span style="font-size: 146px;color:#03283b">{{ 'label.thanks'| trans }}</span></br>
                <span style="font-size: 48px;color:#65a432">{{ 'label.activateaccount.two' | trans }}</span></br>
                <span style="font-size: 32px;color:#65a432;font-family: MyriadProRegular">{{ 'label.activateaccount.three' | trans }}</span>
            </div>

        </div>
            <div class="col-md-12" style="line-height: 50px">
                <span style="font-size: 36px;color:#f59620;font-family: MyriadProRegular">{{ 'label.activateaccount.four' | trans }}</span></br>
                <span style="font-size: 68px;color:#f59620;font-family: MyriadProBold">{{ 'label.activateaccount.five' | trans }}</span>
            </div>
            <a style="color: #0580ff;font-size: 18px" href="{{ url('frontend_partner_home') }}">{{ url('frontend_partner_home') }}</a>
            <div class="col-md-12">
                <span style="font-size: 60px;color:#0a4d77;font-family: MyriadProBoldIt">{{ 'label.activateaccount.six' | trans }}</span>
            </div>


    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script>
        $(document).ready(function () {
            setTimeout(function () {
                window.location.href = "{{ url('frontend_partner_home') }}";
            }, 5000);
        });
    </script>
{% endblock javascripts %}




