{% extends 'LayoutBundle:Layout:layout_partner_frontend.html.twig' %}

{% block carousel %}
{% endblock %}
{% block navbar %}
    {%  include 'PartnerBundle:Layout:navbar.html.twig' with { 'navbar_bg_class' : true }  %}
{% endblock %}
{% block login_register %}
    {% include 'LayoutBundle:Security:overlay_login.html.twig' %}
{% endblock %}
{% block body %}
    <div class="space space-70"></div>
    <div class="container m-t-xl text-center">
        <div class="row text-left" style="margin-left: 11%">
            <img alt="image" class="img-responsive" src="{{ asset('bundles/partner/img/icon/logo_color_'~app.session.get('app_lang_code')|lower)}}.png">
        </div>
        <div class="row m-t-md">
            <div class="col-md-6 col-xs-6">
                <img alt="email_img" class="" src="{{ asset('bundles/partner/img/icon/correo.png')}}"/>
            </div>
            <div class="col-md-6 col-xs-6">
                <span style="font-size: 146px;color:#03283b">{{ 'label.thanks'| trans }}</span>
            </div>
        </div>
        <div class="row text-left">
            <p style="font-size: 24px;color:#64a531;margin-left: 11%">{{ 'label.resgister.description.one' | trans | raw }}</p>
        </div>
        <div class="row">
            <p style="font-size: 72px;color: #094e75">{{ 'label.resgister.description.two' | trans }}</p>
        </div>
    </div>
{% endblock %}




