<div class="ibox card" style="background-color: #0d4e76">
    <div class="ibox-title card" style="background-color: #0d4e76">
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up" style="color: #fff"></i>
            </a>
            <a class="close-link">
                <i class="fa fa-times" style="color: #fff"></i>
            </a>
        </div>
        <div class="row">
            <div class="col-md-4 profile-element">
                {{include('PartnerBundle:Dashboard:img-account.html.twig')}}
                {% if app.user.UserPhoto is not null %}
                    {% set avatar_img =  asset('uploads/userImages/'~app.user.UserPhoto.phoName) %}
                {% else %}
                    {% set avatar_img = asset('bundles/frontend/img/user-no-photo.png') %}
                {% endif %}
                <div class="profile-image">
                    <span class="img-circle dashboard-avatar" style="background: url({{ avatar_img}}) transparent no-repeat scroll center center / cover;display: block;width: 80px;height: 80px;margin-left: 14px">
                    </span>
                </div>
            </div>
            <div class="col-md-8 text-right">
                <span style="color: #fff;font-size: 24px;font-family: MyriadProRegular">{{ 'label.account.agency.one' | trans | raw }}</span></br>
                <span style="color: #fff;font-family: MyriadProLightCondIt; font-size: 23px;font-weight: 600;">{{ 'label.account.agency.two' | trans }}</span>
            </div>
        </div>
    </div>
    <div class="ibox-content card" style="background-color: #0d4e76">
        <form class="form-horizontal form-agency" action="{{ path('frontend_partner_registeragency') }}" method="post" id="form-agency">
          <div class="col-md-12 bg-white m-t-lg">
                <div class="row m-t-md">
                    <div class="col-md-8">
                        <span style="font-size: 24px;font-family: MyriadProBold;color: #1b6592">{{ 'label.data_agency' | trans }}</span>
                    </div>
                    <div class="col-md-4 text-right">
                        <span style="font-size: 18px;font-family: MyriadProBold;color: #1b6592">{{ 'label.save_change' | trans }}</span>
                    </div>
                </div>
                <div class="row agency text-right">
                    <div class="col-md-6">
                        <div class="input-group m-b">
                            <span class="input-group-addon"><i class="glyphicons user"></i></span>
                            {{ form_widget(form.name, { 'attr': {'placeholder': 'label.name.agency'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                        </div>
                        <div class="input-group m-b">
                            <span class="input-group-addon"><i class="glyphicons google_maps"></i></span>
                            {{ form_widget(form.address, { 'attr': {'placeholder': 'label.name.address'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                        </div>
                        <div class="input-group m-b">
                            <span class="input-group-addon"><i class="glyphicons globe_af"></i></span>
                            {{ form_widget(form.country, { 'attr': {'placeholder': 'label.name.country'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="input-group m-b profile-agency-email">
                            <span class="input-group-addon"><i class="glyphicons envelope"></i></span>
                            {{ form_widget(form.email, { 'attr': {'placeholder': 'label.name.email'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control', 'disabled':'true' }}) }}
                        </div>
                        <div class="input-group m-b">
                            <span class="input-group-addon"><i class="glyphicons phone_alt"></i></span>
                            {{ form_widget(form.phone, { 'attr': {'placeholder': 'label.name.phone'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                        </div>
                        <div class="input-group m-b">
                            <span class="input-group-addon"><i class="glyphicons phone_alt"></i></span>
                            {{ form_widget(form.phoneAux, { 'attr': {'placeholder': 'label.name.phone'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                        </div>
                    </div>
                </div>
                <div class="row contact">
                    <div class="col-md-12">
                        <span style="font-size: 24px;font-family: MyriadProBold;color: #64a432">{{ 'label.account.agency.contact' | trans }}</span>
                    </div>
                    <div data-prototype="{% filter escape %}{% include 'PartnerBundle:Prototype:prototype.html.twig' with { 'item': form.contacts.vars.prototype } %}{% endfilter %}"></div>
                    {% for m in form.contacts %}
                        {% include 'PartnerBundle:Prototype:prototype.html.twig' with { 'item': m } %}
                    {% endfor %}
                </div>

            </div>
        </form>
    </div>
</div>
<script  type="text/javascript">
    //Start ImgAccount
    ImgAccount.init();
</script>