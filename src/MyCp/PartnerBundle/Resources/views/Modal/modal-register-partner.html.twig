<div class="modal inmodal fade" id="myModalRegisterAgency" tabindex="-1" role="dialog"  aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header modal-header-agency" style="background-color: #25323b !important;">
                <button data-dismiss="modal" class="close" type="button"><span aria-hidden="true">Ã—</span><span class="sr-only">Close</span></button>
            </div>
            <div class="modal-body modal-agency pb-0">
                <form class="form-horizontal form-agency" action="{{ path('frontend_partner_registeragency') }}" method="post" id="form-agency">

                    <div class="col-md-3  hidden-xs">
                    <div class="row" style="background: url({{ asset('bundles/partner/img/icon/unete_partner_'~app.session.get('app_lang_code')|lower)}}.png)  no-repeat scroll 50%;height: 188px;width:180px;background-size: 100%">
                    </div>
                    <div class="row">
                        <button class="btn btn-primary btn-register-partner">{{ 'label.register.agency' | trans }}</button>
                    </div>
                </div>
                <div class="col-md-9">
                        <div class="row agency text-right">
                            <p class="m-r-12 hidden-xs">{{ 'label.form.agency.one' | trans }}</p>
                            <div class="col-md-6">
                                <div class="inner-addon left-addon">
                                    <i class="glyphicon glyphicons user"></i>
                                    {{ form_widget(form.name, { 'attr': {'placeholder': 'label.name.agency'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                                </div>
                                <div class="inner-addon left-addon">
                                    <i class="glyphicon glyphicons google_maps"></i>
                                    {{ form_widget(form.address, { 'attr': {'placeholder': 'label.name.address'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                                </div>
                                <div class="inner-addon left-addon">
                                    <i class="glyphicon glyphicons globe_af"></i>
                                    {{ form_widget(form.country, { 'attr': {'data-placeholder': 'label.name.country'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control chosen-select-deselect' }}) }}
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="inner-addon left-addon">
                                    <i class="glyphicon glyphicons envelope"></i>
                                    {{ form_widget(form.email, { 'attr': {'placeholder': 'label.name.email'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                                </div>
                                <div class="inner-addon left-addon">
                                    <i class="glyphicon glyphicons phone_alt"></i>
                                    {{ form_widget(form.phone, { 'attr': {'placeholder': 'label.name.phone'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                                </div>
                                <div class="inner-addon left-addon">
                                    <i class="glyphicon glyphicons phone_alt"></i>
                                    {{ form_widget(form.phoneAux, { 'attr': {'placeholder': 'label.name.phone'|trans,'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                                </div>
                            </div>
                        </div>
                        <div class="row text-right">
                            <p class="m-r-12">{{ 'label.form.agency.two' | trans }}</p>
                            <div class="contact" data-prototype="{% filter escape %}{% include 'PartnerBundle:Prototype:prototype.html.twig' with { 'item': form.contacts.vars.prototype } %}{% endfilter %}"></div>
                        </div>
                        <div class="row agency text-right" >
                            <p class="m-r-12">{{ 'label.form.agency.three' | trans }}</p>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="inner-addon left-addon">
                                        <i class="glyphicon glyphicons package"></i>
                                        <select required="required" aria-required="true" aria-describedby="partner_agency_plan-error" name="packageSelect" id="packageSelect" data-placeholder="{{ 'title.column_one'|trans }}" data-toggle="tooltip" data-container="body" class="form-control chosen-select-deselect">
                                            {% for package in packages %}
                                            <option value="{{ package.id }}">{{ package.name }}</option>
                                            {% endfor %}
                                        </select>
                                        <span id="partner_agency_plan-error" class="has-error"></span>

                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="inner-addon left-addon">
                                        <i class="glyphicon glyphicons keys"></i>
                                        <input placeholder="{{ 'label.password' | trans }}" type="password" class="form-control" name="password" id="password" aria-required="true" aria-invalid="false">
                                    </div>
                                    <div class="inner-addon left-addon">
                                        <i class="glyphicon glyphicons keys"></i>
                                        <input placeholder="{{ 'label.password.repeat' | trans }}" type="password" class="form-control" name="confirm" id="confirm" aria-required="true" aria-invalid="true">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="checkbox checkbox-info" style="margin-left: 40px">
                                <input id="checkbox4" type="checkbox" name="terms">
                                <label for="checkbox4" style="font-size: 16px;font-family: MyriadProRegular">
                                    {{ 'label.check.term' | trans | raw }} <a href="{{ path('frontend_partner_legal') }}" target="_blank"> {{ 'label.check.term2' | trans | raw }}</a>
                                </label>
                            </div>
                        </div>
                        <div class="row visible-xs">
                            <button class="btn btn-primary btn-register-partner">{{ 'label.register.agency' | trans }}</button>
                        </div>
                </div>
                </form>
            </div>
            <div class="modal-footer" style="background-color: #25323b !important;border-top: 0px">
            </div>
        </div>
    </div>
</div>