{% set formFilter = form_search_booking_reserved.createView() %}

<div class="ibox card">
    <div class="ibox-title card">
        <div id="appended">
        <span class="title-card" id="title-card">{{ 'label.accounting.account'|trans }} ${{ balance }} {{ coin|lower }}</span>
        </div>
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
            </a>
            <a class="close-link">
                <i class="fa fa-times"></i>
            </a>
        </div>
    </div>
    {% if balance <= 0 %}
    <div class="alert alert-danger" role="alert">
        <div class="glyphicon glyphicon-alert" style="padding-left: 10px;padding-right: 10px"></div>
        <b style="font-size: large"> {{ 'label.accounting.alert.danger'|trans }}</b>
    </div>
    {% elseif balance>=500 and balance <= 800 %}
    <div class="alert alert-warning" role="alert">
        <div class="glyphicon glyphicon-exclamation-sign" style="padding-left: 10px;padding-right: 10px"></div>
       <b style="font-size: large"> {{ 'label.accounting.alert.warning'|trans }}</b>
    </div>
    {% elseif balance>0 and balance <= 500 %}
        <div class="alert alert-danger" role="alert">
            <div class="glyphicon glyphicon-exclamation-sign" style="padding-left: 10px;padding-right: 10px"></div>
           <b style="font-size: large">{{ 'label.accounting.alert.lesdanger'|trans }}</b>
        </div>
    {% endif %}
    <div class="ibox-content card">

        <form id="form-accounting" class="filter-form" >
            <div class="row">



                <div class="col-lg-2 field-filter">
                    <div class="form-group">
                        <div class="input-group date">
                            {{ form_widget(formFilter.from, { 'attr': {'placeholder': 'label.accounting.from'|trans, 'title':'label.accounting.from'|trans, 'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        </div>
                    </div>
                </div>


                <div class="col-lg-2 field-filter ">
                <div class="form-group">
                <div class="input-group date">
                {{ form_widget(formFilter.to, { 'attr': {'placeholder': 'label.accounting.to'|trans, 'title':'label.accounting.to'|trans, 'data-toggle':'tooltip', 'data-container':'body', 'class':'form-control' }}) }}
                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                </div>
                </div>
                </div>


                {#<div class="col-lg-2 field-filter pull-right">#}
                    {#<div class="btn-group m-t-n-xs pull-right">#}
                        {#<a class=""><i class="glyphicons refresh" style="margin-bottom: 10px; color: #0d4e76 !important;"></i> Actualizar</a>#}


                    {#</div>#}
                {#</div>#}




            </div>

            <div class="row">
                <div class="col-lg-2 field-filter pull-right">

                    <div class="btn-group m-t-n-xs pull-right">
                        <div class="btn-group m-t-n-xs pull-right"><a class="btn btn-sm btn-search" data-toggle="modal" title="{{'label.accounting.new.deposit'|trans  }} " data-target="#deposit"> {{'label.accounting.new.deposit'|trans  }} </a></div>
                        <div class="btn-group m-t-n-xs pull-right"><a class="btn btn-sm btn-search" data-toggle="modal" title="{{ 'label.accounting.conciliation'|trans }}"  onClick="showconcil(this)"> {{ 'label.accounting.conciliation'|trans }} </a></div>
                        {#<div class="btn-group m-t-n-xs pull-right"><a class="btn btn-sm btn-search" onClick="clearProccessAction(this)"> Exportar a Excel </a></div>#}
                        <div class="btn-group m-t-n-xs pull-right" style="margin-left: 5px"><a class="btn btn-sm btn-search" onClick="filterAccountingAction(this)"> {{ 'btn.search' | trans }} </a></div>



                    </div>
                </div>
            </div>
        </form>


        <div class="row" style="margin-top: 10px">
            <div class="col-sm-12" style="padding-left: 4px; padding-right: 4px;">
                <div class="ibox">
                    <div class="ibox-content">

                        <div class="table-responsive">

                            <table id="table-accounting" style="font-size: x-large" class="table table-striped table-bordered" cellspacing="0" width="100%">
                                <thead style="background-color: #0c4d77;color: #ffffff;!important;" >

                                    <th>No.</th>

                                    <th>{{ 'today.date'|trans }}</th>

                                    <th>{{ 'label.accounting.description'|trans }}</th>


                                    <th>{{ 'label.accounting.debit'|trans }}</th>


                                    <th>{{ 'label.accounting.credit'|trans }}</th>


                                    <th><b>{{ 'label.accounting.balance'|trans }}</b></th>

                                    <th style="background: #0a4d77 !important;"></th>

                                </thead>
                                <tfoot>
                                <tr>
                                    <th></th>
                                    <th></th>
                                    <th style="text-align:right">Subtotal:<br>Total:</th>
                                    <th></th>
                                    <th ></th>
                                    <th ></th>
                                    <th ></th>
                                </tr>

                                </tfoot>

                            </table>

                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal inmodal fade" id="deposit" tabindex="-1" role="dialog"  aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header modal-header-agency" style="background-color: #25323b !important;">
                <button data-dismiss="modal" class="close" type="button"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            </div>
            <div class="modal-body modal-agency">
                <div class="row text-center">
                    <p class="m-r-12">{{ 'label.accounting.register.deposit' |trans}}</p>
                </div>
                <form class="form-horizontal form-agency "  method="post" id="form-deposit">

                    <div class="col">

                        <div class="row text-right">

                            <div class="contact">
                                <div class="col-md-3">
                                    <div class="inner-addon left-addon">
                                        <i class="glyphicon glyphicons calendar"></i>
                                        <div class="input-group date">
                                            <input type="text" id="date" name="date" required="required" class="form-control" placeholder="{{ 'label.accounting.from'|trans }}"  data-toggle="tooltip" data-container="body">
                                            <span class="input-group-addon hide"><i class="fa fa-calendar"></i></span>

                                        </div>

                                    </div>


                                </div>




                                <div class="col-md-3">
                                    <div class="inner-addon left-addon">
                                        <i class="glyphicon glyphicons money"></i>
                                        <input type="text" name="cash" id="cash" required="required" placeholder="{{ 'label.accounting.deposit'|trans }}" data-toggle="tooltip" data-container="body" class="form-control">
                                    </div>

                                </div>
                            </div>
                            </div>
                        </div>

                        <div class="form-group"> <!-- Message field -->
                            <label class="control-label " for="message">{{ 'label.accounting.description'|trans }}</label>
                            <textarea class="form-control" cols="40" id="message" name="message"   rows="10" style="color: black;"></textarea>
                        </div>


                </form>

            </div>




            <div class="modal-footer" style="background-color: #25323b !important;border-top: 0px">
                <div class="form-group" style="float:  right;">
                    <button class="btn btn-primary " name="submit" onclick="insertDepositAction(this)" >{{ 'label.accounting.register.deposit'|trans }}</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal inmodal fade" id="conciliar" tabindex="-1" role="dialog"  aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header modal-header-agency" style="background-color: #25323b !important;">
                <button data-dismiss="modal" class="close" type="button"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            </div>
            <div class="modal-body modal-agency">
                <div class="row text-center">
                    <p class="m-r-12">{{ 'label.accounting.send.council'|trans }}</p>
                </div>
                <form class="form-horizontal form-agency "  method="post" id="form-conciliar">

                    <div class="col">

                        <div class="row text-center">

                            <div class="contact">
                                <div class="col-md-3">
                                    <div class="inner-addon left-addon">
                                        <i class="glyphicon glyphicons calendar"></i>
                                        <div class="input-group date">
                                            <input type="text" id="date_from" name="date_from" required="required" class="form-control" placeholder="{{ 'label.accounting.from' |trans}}"  data-toggle="tooltip" data-container="body">
                                            <span class="input-group-addon hide"><i class="fa fa-calendar"></i></span>

                                        </div>

                                    </div>


                                </div>




                                <div class="col-md-3">
                                    <div class="inner-addon left-addon">
                                        <i class="glyphicon glyphicons calendar"></i>
                                        <div class="input-group date">
                                            <input type="text" id="date_to" name="date_to" required="required" class="form-control" placeholder="{{ 'label.accounting.to'|trans }}"  data-toggle="tooltip" data-container="body">
                                            <span class="input-group-addon hide"><i class="fa fa-calendar"></i></span>

                                        </div>

                                    </div>

                                </div>

                            </div>
                        </div>

                    </div>

                    <div class="form-group" id="append"> <!-- Message field -->
                    <div id="deleted">

                    </div>

                    </div>
                    <div class="row">
                        <div class="checkbox checkbox-info" style="margin-left: 40px">
                            <input id="terms" type="checkbox" name="terms">
                            <label for="checkbox4" style="font-size: 16px;font-family: MyriadProRegular">
                            {{ 'label.accounting.check'|trans }}
                            </label>
                        </div>
                    </div>

                </form>

            </div>




            <div class="modal-footer" style="background-color: #25323b !important;border-top: 0px">

                <div class="form-group" style="float:  right;">
                    <button class="btn btn-primary " name="submit" onclick="sendConcilAction(this)" >{{ 'label.accounting.send'|trans }}</button>
                </div>
            </div>
        </div>
    </div>
</div>
<style>
    .sorting_disabled {
        background: #0a4d77 !important;
    }
    .buttons-excel{
        font-size: large;!important;
        margin-top: 5px;!important;
        background-color: #d6d7da;!important;
         /*border-color: #1ab394; !important;*/
        border-top-right-radius: 0;
        border-bottom-right-radius: 0;
        margin-left: 0;
        position: relative;
        float: left;
        color: #64a433;!important;
    }

    .buttons-pdf{

        font-size: medium;!important;
        margin-top: 5px;!important;
        background-color: #d6d7da;!important;
        /*border-color: #1ab394; !important;*/
        border-top-right-radius: 0;
        border-bottom-right-radius: 0;
        margin-left: 0;
        position: relative;
        float: left;
        color: #64a433;!important;
    }
</style>
<script type="text/javascript">
    var oTableReserved = '';



    $(document).ready(function () {
        $("#date_from").bind("change paste keyup", function() {
            loadforcouncil();
        });
        $("#date_to").bind("change paste keyup", function() {
            loadforcouncil();
        });
        var currentdate = new Date();
        var datetime = currentdate.getDate() + "/"
            + (currentdate.getMonth()+1)  + "/"
            + currentdate.getFullYear() + " @ "
            + currentdate.getHours() + ":"
            + currentdate.getMinutes() + ":"
            + currentdate.getSeconds();




        oTableReserved = $('#table-accounting').DataTable({
            "language": {
                "url": "{{ asset('bundles/layout/js/plugins/dataTables/locale/') }}{{ app.request.locale }}.json"
            },
            dom: 'Bfrtip',
           
            lengthChange: false,
            buttons: [

                {
                    extend: 'pdfHtml5',
                    title: 'Accounting-'+datetime + '' +
                    ' Tour Operator:'+ "{{ app.user.getUserUserName }}",

                    customize: function(doc) {


                        doc.content.splice( 0, 0, {
                            margin: [ 0, 0, 0, 5 ],
                            alignment: 'right',

                            image: 'data:image/jpeg;base64,',
                            fit: [90, 60]
                        } );

                        doc.styles.tableFooter = {
                            fillColor:"#64a433",

                        }

                    },
                    footer: true,

                    exportOptions : {
                        stripNewlines: false,
                        format : {
                            footer : function (data, column, row){

                               return data.replace(/<br\s*[\/]?>/gi,'\n');
                            }
                        }


                    }
                }
            ],
            bServerSide: false,
            "bFilter": false,
            "bsortable":false,
            'orderClasses': false,
            "bLengthChange": false,
            "scrollX": false,
            'ordering':  false
            ,
            "ajax": {
                "url": "{{ path('partner_dashboard_sumary_accounting') }}",
                 data: function (params) {

                    var inputs = $("#form-accounting input,select");

                    inputs.each(function () {

                        var input = $(this);
                        var val = input.val();
                        var name = input.attr('name');
                        if (val && val != '' && name && name != '') {
                            params[input.attr('name')] = val;
                        }
                    });

                },
                "dataSrc":"",
                 error: function (xhr, error, thrown) {
                    console.log( 'Error' );
                }

            },
            "aoColumnDefs": [
                {
                    "orderable": false,
                    "mData": null,
                    "aTargets": [6],
                    "mRender": function (data, type, full) {

                        if(data.cas!=undefined) {
                            var html = '<a data-href="{{ path("partner_dashboard_booking_detail") }}/' + data.cas + '" onClick="executeAjaxEle(this)"> <i class="glyphicons eye_open" style="margin-bottom: 10px;float: right; color: #0d4e76 !important;">  </i> </a>';
                            return html;
                        }
                        else {
                            return '';
                        }
                    }
                }
            ],
            "columns": [
                { "data": "no" },
                { "data": "created" },
                { "data": "description" },
                { "data": "debit" },
                { "data": "credit" },
                { "data": "balance" }
            ],
            "footerCallback": function ( row, data, start, end, display ) {
                var api = this.api(), data;

                // Remove the formatting to get integer data for summation
                var intVal = function ( i ) {
                    return typeof i === 'string' ?
                        i.replace(/[\$,]/g, '')*1 :
                        typeof i === 'number' ?
                            i : 0;
                };

                // Total over all pages
                total = api
                    .column(3)
                    .data()
                    .reduce( function (a, b) {
                        return intVal(a) + intVal(b);
                    }, 0 );

                // Total over this page
                pageTotal = api
                    .column( 3, { page: 'current'} )
                    .data()
                    .reduce( function (a, b) {
                        return intVal(a) + intVal(b);
                    }, 0 );


                total1 = api
                    .column( 4 )
                    .data()
                    .reduce( function (a, b) {
                        return intVal(a) + intVal(b);
                    }, 0 );

                // Total over this page
                pageTotal1 = api
                    .column( 4, { page: 'current'} )
                    .data()
                    .reduce( function (a, b) {
                        return intVal(a) + intVal(b);
                    }, 0 );

                // Update footer
                $( api.column( 4 ).footer() ).html(
                    '$'+roundTo(total1,2)
                );

                // Update footer
                $( api.column( 3 ).footer() ).html(
                    '$'+total +'</br>'+'$'+roundTo((total-total1),2)
            );

            },

            fnDrawCallback: function() {


            },
            initComplete: function () {
                $('.buttons-pdf').html('<span class="glyphicon glyphicon-file" data-toggle="tooltip" title="Export To Pdf"/>')
            }
        });


    });

    function loadforcouncil() {
        var url= "{{ path('partner_dashboard_sumary_accounting') }}";
        HoldOn.open();

        var date=$('#date_from').val();
        var date_to=$('#date_to').val();


        $.ajax({
            type:"GET",
            url:url,
            data:{'booking_reserved_filter_form':{'from':date,'to':date_to}},
            success:function (response) {

                if(response){
                    $('#deleted').remove();
                    var debe=0;
                    var haber=0;
                    var saldo=0;



                    $.each(
                        response,
                        function(index, value) {
                        if(value.debit!=null&&value.debit!=''){

                            debe+=parseFloat(value.debit);

                        }
                        else{

                            haber+=parseFloat(value.credit);

                        }

                        }
                    );

                    saldo = debe-haber;
                    var html='<div id="deleted" class="row text-center" style="padding-top: 15px">' +
                            '<table  style="border-radius: 5px;\n' +
                        '    width: 100%;\n' +
                        '    float: none;">'+
                            '<thead>'+
                            '<th style="text-align: center;">'+
                            "{{ 'label.accounting.debit'|trans }}" +
                            '</th>'+
                            '<th style="text-align: center;">'+
                            "{{ 'label.accounting.credit' |trans}}"+
                             '</th>'+
                            '<th style="text-align: center;">'+
                            "{{ 'label.accounting.balance'|trans }}"+
                           '</th>'+
                            '</thead>'+
                            '<tbody>'+
                            ' <td id="debit" value="'+debe+'">'+roundTo(debe,2)+'</td>'+
                          ' <td id="credit" value="'+haber+'">'+roundTo(haber,2)+'</td>'+
                         ' <td id="balance" value="'+saldo+'">'+roundTo(saldo,2)+'</td>'+

                            '</tbody>'+
                         '</table>'+
                        '</div>';
                    $('#append').append(html);
                    HoldOn.close();

                }
                else{
                    HoldOn.close();
                }
            }
        });
    }
    function filterAccountingAction() {
        HoldOn.open();
        oTableReserved.ajax.reload();
        HoldOn.close();
    }
    function showconcil() {
        loadforcouncil();
        $('#conciliar').modal('toggle');


    }

    function insertDepositAction() {
       var validator= $("#form-deposit").validate({
            submitHandler: function(form) {
                //form.submit();
            },
            errorElement: 'span',
            errorClass: 'has-error',
            errorPlacement: function(error, element) {
                if (element.attr("name") == "fname" || element.attr("name") == "lname" ) {
                    error.insertAfter("#lastname");
                } else {
                    error.insertAfter(element);
                }
            },
            ignore: "",
            invalidHandler: function(event, validator) { //display error alert on form submit
            },
            rules: {
                'date':{
                    required: true,
                    date: true
                },
                'cash': {
                    required: true,
                    number: true,
                    min: 1
                },
                'message':{
                    required:true
                }
            },
            highlight: function (element, clsError) { // hightlight error inputs
                element = $(element);
                element.parent().addClass(clsError);
            },
            unhighlight: function (element, clsError) { // revert the change done by hightlight
                element = $(element);
                element.parent().removeClass(clsError);
            },
            success: function(label) {
                label.closest('.input-group').removeClass('has-error');
                label.remove();
            }
        });
        if(!validator.form()){
            return;
        }
        var url= "{{ path('partner_dashboard_sumary_accounting_add_deposit') }}";
        HoldOn.open();
        var date=$('#date').val();
        var description=$('#message').val();
        var cash=$('#cash').val();
        $.ajax({
            type:"GET",
            url:url,
            data:{'date':date,'description':description,'cash':cash},
            success:function (response) {

                if(response.success){

                    oTableReserved.ajax.reload();
                    HoldOn.close();
                    $('#deposit').modal('hide');
                }
                else{
                    HoldOn.close();
                }
            }
        });




    }
    function sendConcilAction() {
        var validator= $("#form-conciliar").validate({
            submitHandler: function(form) {
                //form.submit();
            },
            errorElement: 'span',
            errorClass: 'has-error',
            errorPlacement: function(error, element) {
                if (element.attr("name") == "fname" || element.attr("name") == "lname" ) {
                    error.insertAfter("#lastname");
                } else {
                    error.insertAfter(element);
                }
            },
            ignore: "",
            invalidHandler: function(event, validator) { //display error alert on form submit
            },
            rules: {
                'date_from':{
                    required: true,
                    date: true
                },
                'date_to':{
                    required: true,
                    date: true
                },

                'terms':{
                    required:true
                }
            },
            highlight: function (element, clsError) { // hightlight error inputs
                element = $(element);
                element.parent().addClass(clsError);
            },
            unhighlight: function (element, clsError) { // revert the change done by hightlight
                element = $(element);
                element.parent().removeClass(clsError);
            },
            success: function(label) {
                label.closest('.input-group').removeClass('has-error');
                label.remove();
            }
        });
        if(!validator.form()){
            return;
        }
        var url= "{{ path('partner_dashboard_sumary_accounting_conciliation') }}";
        HoldOn.open();
        var from=$('#date_from').val();
        var to=$('#date_to').val();
        var debit=$('#debit').text();
        var credit=$('#credit').text();
        var balance=$('#balance').text();
        $.ajax({
            type:"GET",
            url:url,
            data:{'from':from,'to':to,'debit':debit,'credit':credit,'balance':balance},
            success:function (response) {

                if(response.success){


                    HoldOn.close();
                    $('#conciliar').modal('hide');
                }
                else{
                    HoldOn.close();
                }
            }
        });




    }
    function roundTo(n, digits) {
        if (digits === undefined) {
            digits = 0;
        }

        var multiplicator = Math.pow(10, digits);
        n = parseFloat((n * multiplicator).toFixed(11));
        return Math.round(n) / multiplicator;
    }


</script>