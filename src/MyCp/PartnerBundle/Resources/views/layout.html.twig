<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <link type="image/x-icon" href="{{ asset('bundles/frontend/img/favicon.png') }}" rel="icon">
    <link type="image/x-icon" href="{{ asset('bundles/frontend/img/favicon.png') }}" rel="shortcut icon">

    <title>{% block title %}Home{% endblock %} | Coupon</title>

    {% block stylesheets %}
        {% stylesheets
        '@PartnerBundle/Resources/public/css/plugins/OverlayStyles/style8.css'
        '@PartnerBundle/Resources/public/css/bootstrap.min.css'
        '@PartnerBundle/Resources/public/css/animate.css'
        '@PartnerBundle/Resources/public/font-awesome/css/font-awesome.min.css'
        '@PartnerBundle/Resources/public/css/style.css'
        '@PartnerBundle/Resources/public/css/plugins/datapicker/datepicker3.css'
        '@PartnerBundle/Resources/public/css/plugins/chosen/chosen.css'
        '@PartnerBundle/Resources/public/css/plugins/sweetalert/sweetalert.css'
        '@PartnerBundle/Resources/public/css/plugins/holdon/HoldOn.min.css'
        '@PartnerBundle/Resources/public/css/plugins/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css'
        '@PartnerBundle/Resources/public/css/normalize.css'
        '@PartnerBundle/Resources/public/css/plugins/toastr/toastr.min.css'
        filter='?uglifycss' %}
        <link type="text/css" rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}

    {% endblock %}

</head>
<body id="page-top" class="{% block body_class %}landing-page{% endblock %}">

{% block navbar %}
    {{ include('PartnerBundle:Layout:navbar.html.twig') }}
{% endblock %}
{% block carousel %}
    {{ include('PartnerBundle:Layout:carousel.html.twig') }}
{% endblock %}
{% block login_register %}

{% endblock %}
{% block body %}
{% endblock %}
{% block footer %}
    <div id="adjust-footer-section" class="clearfix"></div>
    {{ include('PartnerBundle:Layout:footer.html.twig') }}
{% endblock %}
{% block javascripts %}
    {% javascripts
    '@PartnerBundle/Resources/public/js/jquery-2.1.1.js'
    '@PartnerBundle/Resources/public/js/bootstrap.min.js'
    '@PartnerBundle/Resources/public/js/plugins/metisMenu/jquery.metisMenu.js'
    '@PartnerBundle/Resources/public/js/plugins/slimscroll/jquery.slimscroll.min.js'
    '@PartnerBundle/Resources/public/js/plugins/pace/pace.min.js'
    '@PartnerBundle/Resources/public/js/plugins/wow/wow.min.js'
    '@PartnerBundle/Resources/public/js/plugins/sweetalert/sweetalert.min.js'
    '@PartnerBundle/Resources/public/js/plugins/chosen/chosen.jquery.js'
    '@PartnerBundle/Resources/public/js/classie.js'
    '@PartnerBundle/Resources/public/js/plugins/OverlayStyles/modernizr.custom.js'
    '@PartnerBundle/Resources/public/js/plugins/OverlayStyles/classie.js'
    '@PartnerBundle/Resources/public/js/plugins/OverlayStyles/page-overlay.js'
    '@PartnerBundle/Resources/public/js/adjustContainer/adjustHeight.js'
    '@PartnerBundle/Resources/public/js/plugins/toastr/toastr.min.js'
    filter='?uglifyjs2' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% if app.request.locale != 'en' %}
    <script src="{{ asset('bundles/frontend/js/plugins/jquery-validation-1.13.1/dist/localization/messages_') }}{{ app.request.locale }}.js"></script>
    {% endif %}
{% endblock %}
</body>
</html>
