{% extends 'MyCpMobileFrontendBundle::base.html.twig' %}
{% set start_date = (post['reservation_filter_date_from'] is defined) ? ( post['reservation_filter_date_from']) : ((app.session.get('search_arrival_date') == null) ? "now" |date("d/m/Y") : app.session.get('search_arrival_date')|date("d/m/Y")) %}
{% set end_date = (post['reservation_filter_date_to'] is defined) ? (post['reservation_filter_date_to']) : ((app.session.get('search_departure_date') == null) ? "+2 days" |date("d/m/Y") : app.session.get('search_departure_date')|date("d/m/Y")) %}

{% set image = ((gallery_photos[0] is defined) ? gallery_photos[0].photo_name : "no_photo.png") %}

{% set promoPrice = (ownership.completePrice != null and ownership.completePrice > 0) ? ownership.completePrice : ownership.ownminimumprice %}
{% set nightPriceTextTag = (ownership.completePrice != null and ownership.completePrice > 0) ? "BYNIGHT" :  "NIGHT_PER_ROOM" %}
{% set hasCompleteReservation = (ownership.completePrice != null and ownership.completePrice > 0)  %}

{% set section = constant('\\MyCp\\mycpBundle\\Entity\\metaTag::SECTION_ACCOMMODATIONS') %}
{% block title %}{{ ownership.ownname }}{% endblock %}
{% block include_css %}
    <style>
    .animated {
    -webkit-transition: height 0.2s;
    -moz-transition: height 0.2s;
    transition: height 0.2s;
    }

    .stars
    {

    font-size: 35px;
    color: #d17581;
    }</style>
    {% endblock %}
{% block metas %}
    <meta name="description" content="{{ ownership.owntype  | trans }} {{ "IN" | trans }} {{ ownership.ownAddressProvince }}: {{ ownership.ownname }} ({{ ownership.mycpcode }}). {% if rooms is defined and rooms | length %} {{ rooms | length }} {% if rooms | length == 1 %} {{ "ROOM_SINGULAR" | trans }} {% else %} {{ "ROOM_PLURAL" | trans }} {% endif %}. {% endif %} {{ "FROM_PRICES" | trans }} {{ promoPrice | price }} {{ nightPriceTextTag | trans }}">
    <meta name="keywords" content="{{ ownership.owntype }} {{ ownership.owntype | trans }} {{ ownership.ownaddressmunicipality }} {{ ownership.ownAddressProvince }} Cuba {{ ownership.ownname }}">
    <!-- Propiedades Open Graph -->
    <meta property="og:title" content="{{ ownership.ownname }} - MyCasaParticular">
    <meta property="og:type" content="website">
    <meta property="og:image" content="{{ app.request.getSchemeAndHttpHost() ~ asset('uploads/ownershipImages/') ~ image }}">
    <meta property="og:author" content="ABUC media network GmbH">
    <meta property="og:site_name" content="MyCasaParticular">
    <meta property="og:description" content="{{ ownership.owntype  | trans }} {{ "IN" | trans }} {{ ownership.ownAddressProvince }}: {{ ownership.ownname }} ({{ ownership.mycpcode }}). {% if rooms is defined and rooms | length %}{{ rooms | length }} {% if rooms | length == 1 %}{{ "ROOM_SINGULAR" | trans | trim }}{% else %}{{ "ROOM_PLURAL" | trans | trim }} {% endif %}{% endif %}. {{ "FROM_PRICES" | trans }} {{ promoPrice | price }} {{ nightPriceTextTag | trans }}">
    <meta property="og:see_also" content="{{ mail_base_url }}">
    <!--meta property="og:locale" content="en_US"/-->
    <meta property="og:url" content="{{ url('frontend_details_ownership',{'own_name':ownership.ownname  | urlNormalize}) }}">
    <meta property="fb:admins" content="312774092497">

    {% set page = app.request.query.get('page')  %}
    <link rel="canonical" href="{{ url("frontend_details_ownership", {'own_name':ownership.ownname|lower|replace(' ','-'), "page": page, "locale": locale}) }}"/>

    {% for lang in allLanguages %}
        {% if lang.langCode | lower != locale %}
            <link rel="alternate" hreflang="{{ langCountry[lang.langCode|lower] }}" href="{{ url("frontend_details_ownership", {'own_name':ownership.ownname|lower|replace(' ','-'), "page": page, "locale": lang.langCode | lower}) }}"/>
        {% endif %}
    {% endfor %}

    {% set next = page + 1 %}
    {% set prev = page - 1 %}
    {% if page == "" or page == 1 %}
        <link rel="next" href="{{ path("frontend_details_ownership", {'own_name':ownership.ownname|lower|replace(' ','-'), "page": next}) }}" />
    {% elseif page > 1 and page < lastPage %}
        <link rel="next" href="{{ path("frontend_details_ownership", {'own_name':ownership.ownname|lower|replace(' ','-'), "page": next}) }}" />
        <link rel="prev" href="{{ path("frontend_details_ownership", {'own_name':ownership.ownname|lower|replace(' ','-'), "page": prev}) }}" />
    {% elseif page == lastPage %}
        <link rel="prev" href="{{ path("frontend_details_ownership", {'own_name':ownership.ownname|lower|replace(' ','-'), "page": prev}) }}" />
    {% endif %}
{% endblock %}
