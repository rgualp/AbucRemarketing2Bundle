{% spaceless %}
    {% set lang_code=app.session.get('app_lang_code')|lower %}
    <!DOCTYPE html>
    <html id="top" {{ get_lang(lang_code)|raw }}>
    <head>
        <title>{% block title %}{{"WELCOMEPAGE_TITLE" | trans }}{% endblock %} *** MyCasaParticular</title>

        {% block metas %}
            {{ get_metas('Main', lang_code)|raw }}
        {% endblock metas %}
        {% stylesheets
            '@MyCpMobileFrontendBundle/Resources/public/css/bootstrap.min.css'
            '@FrontEndBundle/Resources/public/css/plugins/font-awesome/css/font-awesome.min.css'
            '@FrontEndBundle/Resources/public/css/glyphicons.css'
            '@MyCpMobileFrontendBundle/Resources/public/css/ionicons.css'
            '@MyCpMobileFrontendBundle/Resources/public/css/myicons.css'
            '@FrontEndBundle/Resources/public/css/custom.css'
            '@MyCpMobileFrontendBundle/Resources/public/css/style.css'
        filter='?uglifycss'
        %}
        {#'@MyCpMobileFrontendBundle/Resources/public/css/style.css'#}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% if ownership.category is defined and  ownership.category == 'Econ√≥mica'%}
            {% stylesheets
            '@FrontEndBundle/Resources/public/css/economic_house.css'
            filter='?uglifycss'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endif %}
        {% if ownership.category is defined and  ownership.category == 'Premium'%}
            {% stylesheets
            '@FrontEndBundle/Resources/public/css/premium_house.css'
            filter='?uglifycss'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endif %}

