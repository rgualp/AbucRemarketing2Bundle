    <div class="container">
        <div class="row">
            <div class="space space-5"></div>
            <div class="col-xs-7 p-x-1" id="center-cont">
                <div class="space space-5"></div>
                <a href="{{ path('frontend_welcome') }}" class="brand"  title="{{"HOMEPAGE_TITLE" | trans}}">
                    <img src="{{ asset('bundles/frontend/img/mycp.png') }}" style="max-width: 100%" alt="my casa particular"/>
                </a>
            </div>

            {% if app.request.get('_route') %}
                {{ render_esi(url('my_cp_mobile_frontend_top_nav_control', { 'route': app.request.get('_route'), 'routeParams': app.request.get('_route_params')|json_encode|url_encode })) }}
            {% endif %}
        </div>
    </div>
    <nav id="top-menu" role="navigation" class="for-fixed navbar navbar-inverse bg-gradient-blue p-y-0" style="{% if is_home is defined %}border-bottom: solid #98CD34 7px;{% endif %}">
        <div class="navbar-header">
            <button data-target=".navbar-ex1-collapse" data-toggle="collapse" class="navbar-toggle pull-left" type="button">
                <span class="sr-only">Menu</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div id="navbar-top-menu" class="nav navbar-nav pull-left"></div>
            <ul class="nav navbar-nav onhover helvetica-roman pull-right">
                <li>
                    {{ include('@MyCpMobileFrontend/utils/mainMenuUser.html.twig') }}
                </li>
            </ul>
        </div>
        <div class="container">
            {{ include('@MyCpMobileFrontend/utils/navTopBody.html.twig') }}
        </div>
    </nav>

    {% block big_banner %}{% endblock %}
    {% for message in app.session.flashbag.get('message_global_success') %}
        <div class="container">
            <br/>
            <div class="alert alert-info" style="margin-bottom: 10px">
                <button type="button" class="close" data-dismiss="alert">&times;</button>
                {{ message }}
            </div>
        </div>
    {% endfor %}
    {% for message in app.session.flashbag.get('message_global_error') %}
        <div class="container">
            <br/>
            <div class="alert alert-danger" style="margin-bottom: 10px">
                <button type="button" class="close" data-dismiss="alert">&times;</button>
                {{ message }}
            </div>
        </div>
    {% endfor %}
    {% block body %}
        {% block include_javascripts %}{% endblock %}
        {% block include_javascripts_datepicker_message %}

        {% endblock %}
    {% endblock %}
    {% block footer_big %}{% endblock %}

    {% block go_up %}
        <hr>
        <p class="text-center"><a class="scrollToTop" title="{{"GO_UP" | trans }}" href="#"><em class="icon-chevron-up"></em></a></p>
    {% endblock %}

    {% include '@MyCpMobileFrontend/frontend/footer.html.twig' %}

    {% set leavingDate = null %}
    {% if app.session.get('search_departure_date') != null %}
        {% set leavingDate = app.session.get('search_departure_date')|date("d/m/Y") %}
    {% elseif app.session.get('search_arrival_date') != null%}
        % set leavingDate = app.session.get('search_arrival_date')|date_modify("+2 day")|date("d/m/Y") %}
    {% else %}
        {% set leavingDate = "+2 days" |date("d/m/Y") %}
    {% endif %}

    <script type="text/javascript">
        var today_date="{{"now"|date("d/m/Y") }}";
        var start_date="{{ (app.session.get('search_arrival_date') != null) ? (app.session.get('search_arrival_date')|date("d/m/Y")) : ("now"|date("d/m/Y")) }}";
        var end_date="{{leavingDate}}";
    </script>

