    <script type="text/javascript">
        var today_date="{{"now"|date("d/m/Y") }}";

        NavbarScrollFixed.init(".for-fixed","navbar-fixed-top", 80, enrango, fuerarango);

        $('.scrollToTop').click(function(event){
            event.preventDefault();
            jQuery('html, body').animate({scrollTop : 0},800);
            return false;
        });

        function enrango(){
            var _ul = $("#menu-favorites").clone();
            $("#menu-favorites").remove();
            $("#navbar-top-menu").html(_ul);
        }
        function fuerarango(){
            var _ul = $("#menu-favorites").clone();
            $("#menu-favorites").remove();
            $("#menu-favorites-content").html(_ul);
        }
    </script>

