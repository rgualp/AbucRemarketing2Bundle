{%if is_in_favorite is defined and is_in_favorite%}
   <a class="favorite_on" data-url="{{path('frontend_delete_favorite')}}" data-favorite-type="{{favorite_type}}" data-element-id="{{element_id}}" data-list-preffix ="{{list_preffix}}" title="{{'DELETE_FAVORITIES' | trans}}" >
    <img src="{{asset("bundles/frontend/images/empty.png")}}">
    </a>     
{%else%}
    <a class="favorite_off" data-url="{{path('frontend_insert_favorite')}}" data-favorite-type="{{favorite_type}}" data-element-id="{{element_id}}" data-list-preffix ="{{list_preffix}}" title="{{'ADD_FAVORITIES' | trans}}">
    <img src="{{asset("bundles/frontend/images/empty.png")}}">
    </a>  
{%endif%}