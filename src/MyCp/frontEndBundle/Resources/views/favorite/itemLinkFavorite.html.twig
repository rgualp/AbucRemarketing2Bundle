{%if is_in_favorites_list is defined and is_in_favorites_list%}
     <a class="favorite_on delete_from_favorite_list" data-url="{{path('frontend_delete_from_list_favorite')}}" data-favorite-type="{{favorite_type}}" data-element-id="{{element_id}}" data-list-preffix ="{{list_preffix}}" title="{{'DELETE_FAVORITIES' | trans}}" >
        <img src="{{asset("bundles/frontend/images/empty.png")}}">
        </a> 
{%else%}
    {%if is_in_favorite is defined and is_in_favorite%}
    <a class="favorite_on favorite_on_action" data-url="{{path('frontend_delete_favorite')}}" data-favorite-type="{{favorite_type}}" data-element-id="{{element_id}}" data-list-preffix ="{{list_preffix}}" title="{{'DELETE_FAVORITIES' | trans}}" >
        <img src="{{asset("bundles/frontend/images/empty.png")}}">
        </a>     
    {%else%}
        <a class="favorite_off favorite_off_action" data-url="{{path('frontend_insert_favorite')}}" data-favorite-type="{{favorite_type}}" data-element-id="{{element_id}}" data-list-preffix ="{{list_preffix}}" title="{{'ADD_FAVORITIES' | trans}}">
        <img src="{{asset("bundles/frontend/images/empty.png")}}">
        </a>  
    {%endif%}
{%endif%}