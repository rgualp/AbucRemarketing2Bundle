 <div class="secondary_tab row-fluid">
    <div class="nav-tabs_cont">
        <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#favorite_destinations">{{"FAV_DESTINATION" | trans}}</a></li>
            <li><a data-toggle="tab" href="#history_destinations">{{"HISTORY_DESTINATIONS" | trans}}</a></li>
            <li><a data-toggle="tab" href="#favorite_owns">{{"FAV_OWNERSHIPS" | trans}}</a></li>
            <li><a data-toggle="tab" href="#history_owns">{{"HISTORY_OWNERSHIPS" | trans}}</a></li>
        </ul>
    </div>
    <div class="tab-content">
        <div id="favorite_destinations" class="tab-pane fade in active">
            <div class="row-fluid">
                {% for destination in destination_favorites %}
                    <div class="span3">
                {{ include('frontEndBundle:destination:homeItemDestination.html.twig',{'destination': destination, 
                'is_in_favorite': true,
                'photo': destination_favorites_photos[destination.desid],
                'localization' : destination_favorites_localizations[destination.desid],
                'statistics': destination_favorities_statistics[destination.desid],
                'list_preffix': 'destination_favorites'}) }}
                            </div>
                {% else %}
                <div class="alert alert-info" style="margin: 0;">{{"EMPTY_LIST" | trans}}</div>
                {% endfor %}
            </div>
        </div>
        <div id="history_destinations" class="tab-pane fade in">
            <div class="row-fluid">
                {% for destination in history_destinations %}
                    <div class="span3">
                {{ include('frontEndBundle:destination:homeItemDestination.html.twig',
                   {'destination': destination, 
                    'is_in_favorite': history_destinations_favorites[destination.desid],
                    'photo': history_destinations_photos[destination.desid],
                    'localization' : history_destinations_localizations[destination.desid],
                    'statistics': history_destinations_statistics[destination.desid],
                    'list_preffix': 'history_destinations'}) }} 
                            </div>
                {% else %}
                <div class="alert alert-info" style="margin: 0;">{{"EMPTY_LIST" | trans}}</div>
                {% endfor %}
            </div>
        </div>
        <div id="favorite_owns" class="tab-pane fade in">
            <div class="row-fluid">
                {% for ownership in favorite_owns %}
                {{ include('frontEndBundle:ownership:homeItemOwnership.html.twig',
                    {'ownership': ownership, 
                     'is_in_favorite': true,
                     'photo': favorite_owns_photos[ownership.ownid],
                     'list_preffix': 'favorite_owns'}) }} 
                {% else %}
                <div class="alert alert-info" style="margin: 0;">{{"EMPTY_LIST" | trans}}</div>
                {% endfor %}
            </div>
        </div>
        <div id="history_owns" class="tab-pane fade in">
            <div class="row-fluid">
                {% for ownership in history_owns %}
                {{ include('frontEndBundle:ownership:homeItemOwnership.html.twig',
                   {'ownership': ownership, 
                   'is_in_favorite': history_owns_favorites[ownership.ownid],
                   'photo': history_owns_photos[ownership.ownid],
                   'list_preffix': 'history_owns'}) }} 
                {% else %}
                <div class="alert alert-info" style="margin: 0;">{{"EMPTY_LIST" | trans}}</div>
                {% endfor %}
            </div>
        </div>
    </div>
</div>