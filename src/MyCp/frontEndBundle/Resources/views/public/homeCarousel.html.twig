<script type="text/javascript">
    $('.carousel-bottom .nav-tabs li').click(function(){
        move_scroll();
    })

    $('.carousel.slide').on('slid.bs.carousel', function(){
        move_scroll();
    });

    function move_scroll()
    {
        win=$(window);
        win.scrollTop(win.scrollTop()+1);
        win.scrollTop(win.scrollTop()-1);

    }
</script>