{% extends 'frontEndBundle:layout:baseBlank.html.twig' %}
{% block content %}
<div id="big_map_details" class="google-maps" style="width:100%;height: 500px;"
    data-x="{{ownership.OwnGeolocateX}}"
    data-y="{{ownership.OwnGeolocateY}}"
    data-name="{{ownership.ownname}}"
    data-description="{{description | striptags}}"
    data-image="{{asset('uploads/ownershipImages/'~image)}}"
    data-icon1="http://www.mycasaparticular.com/maps.png"
    data-icon="http://{{ app.request.getBaseURL() }}{{asset('/web/bundles/frontend/images/map.png')}}">
</div>
 {% block include_javascripts_and_css %}
    <script src="http://maps.googleapis.com/maps/api/js?v=3&sensor=false" type="text/javascript"></script> 
    <script src="{{ asset('bundles/frontend/js/jquery/jquery.ui.map.js') }}"></script>
    <script src="{{ asset('bundles/frontend/js/jquery/jquery.ui.map.infobox.js') }}"></script>
    <script src="{{ asset('bundles/frontend/js/ownership_details.js') }}"></script>       
{% endblock %}
{% endblock %}