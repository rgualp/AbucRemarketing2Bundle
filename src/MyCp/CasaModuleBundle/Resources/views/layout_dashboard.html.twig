{% extends 'MyCpCasaModuleBundle::base_dashboard.html.twig' %}

    {% block stylesheets %}

        {{ parent() }}
        <title>{% block title %}{{"WELCOMEPAGE_TITLE" | trans | title}}{% endblock %} - MyCasaParticular</title>
        {% stylesheets
        '@MyCpCasaModuleBundle/Resources/public/css/bootstrap.min.css'
        '@MyCpCasaModuleBundle/Resources/public/font-awesome/css/font-awesome.min.css'
        '@MyCpCasaModuleBundle/Resources/public/css/glyphicons.css'
        '@FrontEndBundle/Resources/public/css/ionicons.css'
        '@MyCpCasaModuleBundle/Resources/public/css/iconomycp.css'
        '@MyCpCasaModuleBundle/Resources/public/css/animate.css'
        '@MyCpCasaModuleBundle/Resources/public/css/style.css'
        '@MyCpCasaModuleBundle/Resources/public/css/casa-module.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/chosen/chosen.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/select2/select2.min.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/holdon/HoldOn.min.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/toastr/toastr.min.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/sweetalert/sweetalert.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/switchery/switchery.css'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/fullcalendar/fullcalendar.css'
        filter='?uglifycss'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        <script src="{{ global_google_maps_api_url_with_key }}&amp;libraries=places"></script>

    {% endblock stylesheets %}

    {% block javascripts_first %}

    {% endblock javascripts_first %}

    {% block javascripts %}
        {{ parent() }}
        {% javascripts
        '@MyCpCasaModuleBundle/Resources/public/js/jquery-2.1.1.js'
        '@MyCpCasaModuleBundle/Resources/public/js/bootstrap.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/metisMenu/jquery.metisMenu.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/slimscroll/jquery.slimscroll.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/pace/pace.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/toastr/toastr.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/chosen/chosen.jquery.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/select2/select2.full.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/holdon/HoldOn.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/sweetalert/sweetalert.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/switchery/switchery.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/flot/jquery.flot.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/flot/jquery.flot.tooltip.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/flot/jquery.flot.resize.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/flot/jquery.flot.pie.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/flot/jquery.flot.time.js'
        '@MyCpCasaModuleBundle/Resources/public/js/inspinia.js'
        '@FrontEndBundle/Resources/public/js/jquery-validation/jquery.validate.min.js'
        '@MyCpCasaModuleBundle/Resources/public/css/plugins/bootstrap-wizard/jquery.bootstrap.wizard.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/iCheck/icheck.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/jquery-geocomplete/jquery.geocomplete.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/signals/signals.js'
        '@MyCpCasaModuleBundle/Resources/public/js/plugins/jquery-ui/jquery-ui.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/jquery.fileupload.min.js'
        '@MyCpCasaModuleBundle/Resources/public/js/App.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/step1.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/step2.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/step3.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/step4.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/step5.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/step6.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/step7.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/img-account.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/property.js'
        '@MyCpCasaModuleBundle/Resources/public/js/script/estadisticas.js'
        filter='?uglifyjs2'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <!-- Custom config -->
        {% include 'MyCpCasaModuleBundle::layout_config.html.twig' %}
        <script type="text/javascript" src="{{ asset('bundles/frontend/js/jquery-validation/localization/messages_'~app.request.locale~'.js') }}"></script>

        <script>
            var logoutUrl='{{ url('my_cp_casa_logout') }}';
            var publishUrl='{{ url('casa_publish_ownership') }}';
        </script>

        {% if ownership.modifying %}
            <script>
                $(document).on('ready', function () {
                    $(window).scroll(function(){
                        if($(this).scrollTop()>$('#pub_always').position().top){
                            $('#pub_always').css({position:'fixed',top:0, zIndex:3000});
                        }else{
                            $('#pub_always').css({position:'relative'});
                        }

                    });
                });
            </script>
        {% endif %}
    {% endblock javascripts %}



    {% block body %}
        <div id="wrapper">
            {% block nav %}
                <nav class="navbar-default navbar-static-side" role="navigation">
                  <div class="pull-right" style="position: relative;z-index: 1000;right: 0;">
                     <a class="navbar-minimalize minimalize-styl-2 pull-right nav-label" href="#">
                              {#<i class="collapse_arrow"></i>#}
                         <img src="{{ asset('bundles/mycpcasamodule/img/Zaeta-Izquierda.png') }}">
                     </a>
                  </div>
                    <div class="sidebar-collapse">

                        <ul class="nav metismenu" id="side-menu">

                            {% block profile %}
                                {% include 'MyCpCasaModuleBundle::layout_profile.html.twig' %}
                            {% endblock profile %}

                            {% block menu %}
                                {% include 'MyCpCasaModuleBundle:menu:register.html.twig' %}
                            {% endblock menu %}

                        </ul>
                    </div>
                </nav>
            {% endblock nav %}

            <div id="page-wrapper" class="white-bg" data-idown="{{ ownership.OwnId }}">
                <div class="row border-bottom">
                    <nav class="navbar navbar-static-top  " role="navigation" style="margin-bottom: 0">
                        <div class="navbar-header">
                            <a class="navbar-minimalize minimalize-styl-2 pull-left hidden-sm hidden-md hidden-lg nav-label" href="#">
                                <i class="ico_pinunpin glyphicons left_arrow"></i>
                            </a>
                            <img class="img-mycp img-responsive" src="{{ asset('img/mycp.png') }}" style="margin-top: 5px; margin-left: 25px;">

                        </div>
                        <ul class="nav navbar-top-links navbar-right" style="background-color: #f3f3f3;height: 100%;margin-top: 0px">
                            <li class="dropdown" style="margin-top: 7px">
                                <a class="dropdown-toggle {% if ownership is defined and ownership.ownCategory is not null %}{% if ownership.ownCategory =='Económica'  %}glyphicons white certificate{% endif %}{% if ownership.ownCategory =='Rango medio'  %}glyphicons white cup{% endif %}{% if ownership.ownCategory =='Premium'  %}glyphicons white crown{% endif %}{% endif %}" data-toggle="dropdown" href="#">
                                    <span class="pull-left" style="margin-right: 10px;font-size: 24px">{{ app.user.getName }}</span>
                                    <span class="hidden-xs" style="margin-right: 10px;">{% if ownership is defined and ownership.ownCategory is not null %}{{ ownership.ownCategory }}{% endif %}</span>
                                    <i class="fa fa-angle-down"></i>
                                </a>
                                <ul class="dropdown-menu">
                                    <li>
                                    <a href="{{ path('my_cp_casa_logout') }}">
                                    <i class="fa fa-sign-out"></i> Cerrar sesión
                                    </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>

                    </nav>
                    <div id="show-help-mobile" style="display: none">
                        <div class="fa fa-times-circle" id="close-help"></div>
                        <div class="content-help"></div>
                    </div>
                </div>

                <div id="content_id" class="{% if dashboard is defined and dashboard %}wrapper wrapper-content white-bg{% else %}row white-bg{% endif %}" style="padding-bottom:40px">

                    {% if ownership.modifying %}
                        <div id="pub_always" class="col-xs-12 text-center" style="display: flex">
                            <div class="col-md-9 col-xs-12">
                                <h3 style="background-color: rgb(233, 75, 61); padding: 5px; color: white">OJO Su casa estará inactiva hasta que usted no complete todos los pasos y acceda a la opción Publicar.</h3>
                                <button onclick="Step7.saveProfile(false, true)" class="btn btn-primary dim" type="button" data-url="{{ path("can_publish_accommodation") }}">Publicar <img src="{{ asset('bundles/mycpcasamodule/img/thumbs_up.png') }}"/></button>
                            </div>
                            <div class="col-md-3 oneblue-bg">
                            </div>
                        </div>
                    {% endif %}

                    {% block content %}
                         {% include 'MyCpCasaModuleBundle:Steps:wizard.html.twig' %}
                     {% endblock content %}
                </div>

                {% block footer %}
                    {% include 'MyCpCasaModuleBundle::layout_footer.html.twig' %}
                {% endblock footer %}

            </div>
        </div>
    {% endblock body %}