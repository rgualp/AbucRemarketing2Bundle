{% spaceless %}
{% set lang_code=app.session.get('app_lang_code')|lower %}
<!DOCTYPE html>
<html id="top" {{ get_lang(lang_code)|raw }}>
<head>
    <title>{% block title %}{{"WELCOMEPAGE_TITLE" | trans }}{% endblock %} *** MyCasaParticular</title>

    {% block metas %}
        {{ get_metas('Main', lang_code)|raw }}
    {% endblock metas %}

    {% stylesheets
    '@FrontEndBundle/Resources/public/css/custom.css'

    '@FrontEndBundle/Resources/public/css/glyphicons.css'
    '@MyCpCasaModuleBundle/Resources/public/css/animate.css'
    '@MyCpMobileFrontendBundle/Resources/public/css/ionicons.css'
    '@FrontEndBundle/Resources/public/css/plugins/font-awesome/css/font-awesome.min.css'
    '@FrontEndBundle/Resources/public/css/plugins/iCheck/custom.css'
    '@LayoutBundle/Resources/public/css/plugins/sweetalert/sweetalert.css'
    '@LayoutBundle/Resources/public/css/bootstrap-submenu.css'
    filter='?uglifycss'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% if ownership.category is defined and  ownership.category == 'Econ√≥mica'%}
        {% stylesheets
        '@FrontEndBundle/Resources/public/css/economic_house.css'
        filter='?uglifycss'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endif %}
    {% if ownership.category is defined and  ownership.category == 'Premium'%}
        {% stylesheets
        '@FrontEndBundle/Resources/public/css/premium_house.css'
        filter='?uglifycss'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endif %}

    {#<script src="{{ global_google_maps_api_url_with_key }}" type="text/javascript"></script>#}

    <script>
        var start_date = "{{ (isDateTime(app.session.get('search_arrival_date')) ) ? (app.session.get('search_arrival_date')|date("d/m/Y")) : ("now"|date("d/m/Y")) }}";
        var end_date = "{{ (isDateTime(app.session.get('search_departure_date')) ) ? (app.session.get('search_departure_date')|date("d/m/Y")) : "+2 days" |date("d/m/Y")}}";
    </script>

    {% javascripts
    '@FrontEndBundle/Resources/public/js/jquery.js'
    '@FrontEndBundle/Resources/public/js/bootstrap.min.js'
    '@FrontEndBundle/Resources/public/js/bootstrap-datepicker.js'
    '@FrontEndBundle/Resources/public/js/bootstrap-datepicker.es.js'
    '@FrontEndBundle/Resources/public/js/bootstrap-datepicker.de.js'
    '@LayoutBundle/Resources/public/js/bootstrap-submenu.js'
    '@FrontEndBundle/Resources/public/js/toastr-master/build/toastr.min.js'
    '@FrontEndBundle/Resources/public/js/toastr-master/abuc-toastr.js'
    '@FrontEndBundle/Resources/public/js/respond.min.js'
    '@FrontEndBundle/Resources/public/js/jail.min.js'
    '@FrontEndBundle/Resources/public/js/custom.min.js'
    '@FrontEndBundle/Resources/public/js/destinations.min.js'
    '@FrontEndBundle/Resources/public/js/jquery.royalslider.min.js'
    '@FrontEndBundle/Resources/public/js/typeahead.min.js'
    '@FrontEndBundle/Resources/public/js/ownership.min.js'
    '@FrontEndBundle/Resources/public/js/plugins/iCheck/icheck.min.js'
    '@MyCpMobileFrontendBundle/Resources/public/js/plugins/navbar-scroll-fixed.js'
    '@LayoutBundle/Resources/public/js/plugins/sweetalert/sweetalert.min.js'
    '@LayoutBundle/Resources/public/js/plugins/jquery.unveil.js'
    filter='?uglifyjs2'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% block include_javascripts_and_css %}{% endblock %}
</head>
<body>
{% block custom_menus %}
    <div class="space space-20"></div>
    <div class="container">
        <div class="row">
            <div class="col-sm-3 " id="center-cont">
                <a href="{{ path('frontend_welcome') }}" class="brand"  title="{{"HOMEPAGE_TITLE" | trans}}">
                    <img src="{{ asset('bundles/frontend/img/mycp.png') }}" style="max-width: 100%" alt="my casa particular"/>
                </a>
            </div>

            {% if app.request.get('_route') %}
            {{ render_esi(url('frontend_public_top_nav_control', { 'route': app.request.get('_route'), 'routeParams': app.request.get('_route_params')|json_encode|url_encode })) }}
            {% endif %}
        </div>
    </div>
    <div id="top-menu-cont" style="height: {% block nav_heigth %}{% if is_home is defined %}52px;{% else %}{% if reservation_confirm is defined %}76px{% else %}95px{% endif %}{% endif %}{% endblock %}">
    <nav id="top-menu" role="navigation" class="navbar navbar-inverse bg-gradient-blue" style="{% if is_home is defined %}border-bottom: solid #98CD34 7px;{% endif %}">
        <div class="navbar-header">
            <button data-target=".navbar-ex1-collapse" data-toggle="collapse" class="navbar-toggle" type="button">
                <span class="sr-only">Menu</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div class="container">
            {{ include('FrontEndBundle:utils:navTopBody.html.twig') }}
        </div>
        {% if is_home is not defined %}
            <div class="{% block class_breadcrumbs %} breadcrumb-top {% endblock %}">
                <div class="container">
                    {% block breadcrumbs %}
                        <a href="#">&nbsp;</a>
                    {% endblock %}
                </div>
            </div>
        {% endif %}
    </nav>
</div>
{% endblock %}
{% block big_banner %}{% endblock %}
{% for message in app.session.flashbag.get('message_global_success') %}
    <div class="container">
        <br/>
        <div class="alert alert-info" style="margin-bottom: 10px">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            {{ message }}
        </div>
    </div>
{% endfor %}
{% for message in app.session.flashbag.get('message_global_error') %}
    <div class="container">
        <br/>
        <div class="alert alert-danger" style="margin-bottom: 10px">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            {{ message }}
        </div>
    </div>
{% endfor %}
{% block body %}
    {% block include_javascripts %}{% endblock %}
    {% block include_javascripts_datepicker_message %}

    {% endblock %}
{% endblock %}
{% block footer_big %}{% endblock %}

{% block go_up %}
    <hr>
    <p class="text-center"><a title="{{"GO_UP" | trans }}" href="#"><em class="icon-chevron-up"></em></a></p>
{% endblock %}

<footer class="bg-gradient-blue {% block forsearch %}{% endblock %}">
    <div class="container">
        <div class="row">
            <div class="col-xs-2">
                <h3>{{"DESTINATIONS" | trans}}</h3>
                <ul>
                    <li><a title="My Casa Particular - {{"TOP_DESTINATIONS" | trans}}" href="{{ path('frontend_list_destinations')}}">{{"TOP_DESTINATIONS" | trans}}</a></li>
                    <li><a title="My Casa Particular - {{"MOST_VISITED_CITIES" | trans}}" href="{{path('frontend_get_with_reservations_municipality')}}">{{"MOST_VISITED_CITIES" | trans}}</a></li>
                </ul>
            </div>
            <div class="col-xs-2">
                <h3>{{"ACCOMMODATIONS" | trans}}</h3>
                <ul>
