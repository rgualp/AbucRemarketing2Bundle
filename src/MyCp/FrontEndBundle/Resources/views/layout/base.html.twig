                    <li><a title="My Casa Particular - {{"FAQ" | trans}}" href="{{path('frontend_list_faq')}}">{{"FAQ" | trans}}</a></li>
                    <li><a title="My Casa Particular - {{"LEGAL_TERMS" | trans}}" href="{{path('frontend_legal_terms')}}">{{"LEGAL_TERMS" | trans}}</a></li>
                    <li><a title="My Casa Particular - {{"SECURITY_AND_PRIVACITY" | trans}}" href="{{path('frontend_security_privacity')}}">{{"SECURITY_AND_PRIVACITY" | trans}}</a></li>
                </ul>
            </div>
            <div class="col-xs-2">
                <h3>{{"OUR_SERVICES" | trans}}</h3>
                <ul>
                    <li><a title="My Casa Particular - {{"SITE_MAP" | trans}}"  href="{{ path ('frontend_sitemap_information')}}">{{"SITE_MAP" | trans}}</a></li>
                </ul>
            </div>

            <div class="col-xs-2">
                <div class="col-xs-12">
                    <h3 style="margin-bottom: 5px;">{{"HOUSE_QUESTION" | trans}}</h3>
                    <ul>
                        <li style="font-size: 13px; margin-bottom: 5px;"><a href="{{ path('frontend_mycasarenta_index') }}" >{{ 'MYCASA_RENTA_APP'|trans }}</a></li>
                        {#<li><a href="https://play.google.com/store/apps/details?id=hds.aplications.com.homedroid" target="_blank"><img width="120px" src="{{ asset('bundles/frontend/img/google-play.png') }}"></a> </li>#}
                    </ul>
                </div>
                <div class="col-xs-12">
                    <h3 style="margin-bottom: 5px;">{{"find_reservations" | trans}}</h3>
                    <ul>
                        <li style="font-size: 13px; margin-bottom: 5px;"><a href="{{ path('frontend_mycasatrip_apps') }}" >{{ 'MyCasa Trip APP' | upper }}</a></li>
                    </ul>
                </div>
                <div class="col-xs-12">
                    <h3 style="margin-bottom: 5px;">{{"find_location" | trans}}</h3>
                    <ul>
                        <li style="font-size: 13px; margin-bottom: 5px;"><a href="https://play.google.com/store/apps/details?id=hds.aplications.com.mycubamap" >{{ 'MyCuba Map APP' | upper }}</a></li>
                    </ul>
                </div>
                <div class="col-xs-12">
                    <h3 style="margin-bottom: 5px;">{{"find_partner" | trans}}</h3>
                    <div><span style="background-color: red; margin-left: -15px; padding: 0px 3px;">BETA</span></div>
                    <ul>
                        <li style="font-size: 13px; margin-bottom: 5px;"><a href="{{ path('frontend_partner_home') }}" >{{ 'hazte_partner' | trans | upper }}</a></li>
                    </ul>
                </div>
            </div>

            <div class="col-xs-2 text-right">
                <img id="firm" src="{{ asset('bundles/frontend/img/seal_'~app.session.get('app_lang_code')|lower)}}.png" title="MyCasaParticular.com" alt="{{"MyCasaParticular" | trans}}">
            </div>
        </div>
        <div class="row">
            <div class="col-sm-3">
        <div style="margin: 0 0 10px;">
            <a title="{{"HOMEPAGE_TITLE" | trans}}" href="{{ path('frontend_welcome') }}" >
                <img alt="{{"HOMEPAGE_TITLE" | trans}}" src="{{ asset('bundles/frontend/img/mycp-grey.png')}}">
            </a>
        </div>
            </div>
            <div class="col-sm-4 col-md-3" >

                <div id="social"   class="col-sm-12  text-right">
                    {{ include('FrontEndBundle:utils:netSocials.html.twig') }}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-8" style="margin-bottom: 2em">
                <p>&copy; 2009 - {{'today'|date('Y')}}. <a style="font-size: 11px; color: #ffffff" href="http://www.mycasaparticular.com">MyCasaParticular.com</a>.
                    {{"FOOTER_COPYRIGHT" | trans}}.</p>
            </div>

        </div>
    </div>
</footer>
