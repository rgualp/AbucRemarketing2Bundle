<div class="thumbnail {% if ownership.OwnInmediateBooking %}with-quick-ribbon{% endif %}"
        {% if ownership.OwnInmediateBooking and ownership.OwnInmediateBooking2 %}
            data-text="{{ 'IMMEDIATE_BOOKING'|trans }}"
        {% elseif  ownership.OwnInmediateBooking and not ownership.OwnInmediateBooking2 %}
            data-text="{{ 'quick_bookinhg'|trans }}"
        {% elseif  ownership.OwnInmediateBooking2 and not ownership.OwnInmediateBooking %}
            data-text="{{ 'IMMEDIATE_BOOKING'|trans }}"
        {% endif %}
     style="position:relative" rel="tooltip" title="
    {% if ownership.OwnInmediateBooking and ownership.OwnInmediateBooking2 %}
            {{ "TOOL_TIP_IMMEDIATE_BOOKING" | trans }}
    {% elseif  ownership.OwnInmediateBooking and not ownership.OwnInmediateBooking2 %}
           {{ "quick_booking_tooltip" | trans }}
    {% elseif  ownership.OwnInmediateBooking2 and not ownership.OwnInmediateBooking %}
           {{ "TOOL_TIP_IMMEDIATE_BOOKING" | trans }}
     {% else %}
        {{ "MORE_INFO_PREFFIX" | trans }} {{ name }}
     {% endif %}" xmlns="http://www.w3.org/1999/html">

    {% if ownership.OwnInmediateBooking and ownership.OwnInmediateBooking2 %}
        <div class="quick-ribbon" style="background-color: #ff0000">
            <i class="fa fa-rocket" style="padding: 0px 5px"></i> <span style="text-align: left;">{{ 'IMMEDIATE_BOOKING'|trans }}</span>
        </div>
        {% elseif  ownership.OwnInmediateBooking and not ownership.OwnInmediateBooking2 %}
            <div class="quick-ribbon">
                <i class="glyphicons white electricity icon-padding"></i> <span style="text-align: left;">{{ 'quick_bookinhg'|trans }}</span>
            </div>
        {% elseif  ownership.OwnInmediateBooking2 and not ownership.OwnInmediateBooking %}
            <div class="quick-ribbon" style="background-color: #ff0000">
                <i class="fa fa-rocket" style="padding: 0px 5px"></i> <span style="text-align: left;">{{ 'IMMEDIATE_BOOKING'|trans }}</span>
            </div>
    {% endif %}

    <div style="height: 63px">
        {% if award %}
            <img src="{{ asset("uploads/awards/" ~ award) }}" style="width: 40px; position: relative; z-index: 1;box-shadow: 5px 5px 10px #686765;border-radius:100px" title="{{"Awards" | trans}}" alt="{{"Awards" | trans}}"/>
        {% endif %}
        <span class="h-blue pull-right" style="font-size: 25px"><small style="display: inline-block">{{ "FROM" | trans }}:</small> {{ minPrice | price }}</span>
    </div>
    <a target="_blank" href="{{ path('frontend_details_ownership',{'own_name':name  | urlNormalize}) }}">
        {% set url_image = asset("uploads/ownershipImages/" ~ photo) %}
        {% if notLazyLoad is defined and notLazyLoad == true %}
            <img title="{{ name }}" alt="{{ name }}" class="lazyload" src="{{ url_image | apply_filter('mycp_thumb_front') }}" style="min-height: 200px" />
        {% else %}
            <img title="{{ name }}" alt="{{ name }}" id="image_acc{{ id }}" class="bttrlazyloading" data-bttrlazyloading-md-src="{{ url_image | apply_filter('mycp_thumb_front') }}" />
        {% endif %}
    </a>
</div>

<div class="row">
    <a target="_blank"  href="{{ path('frontend_details_ownership',{'own_name':name  | urlNormalize}) }}">
    <div class="col-sm-8 h-blue">
        {{ name| title }}</br> <span style="font-size: 12px;margin-top: -6px;position: absolute;">{{ destination }}</span>
    </div>
    </a>
    <div class="col-sm-4 text-right" style="margin-top: 20px;">
        {{ include('FrontEndBundle:favorite:itemFavorite.html.twig',
        {'is_in_favorite': is_in_favorite,
        'favorite_type': 'ownership',
        'element_id':id,
        'list_preffix' : list_preffix,
        'include_text': false
        }) }}
        {{ include('FrontEndBundle:cart:addCart.html.twig',{'idOwnership':ownership.own_id}) }}
    </div>
</div>
