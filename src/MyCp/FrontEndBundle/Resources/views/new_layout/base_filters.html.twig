{% for message in app.session.flashbag.get('message_global_success') %}
<div class="container">
    <br/>
    <div class="alert alert-info" style="margin-bottom: 10px;top: 64px;">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        {{ message }}
    </div>
</div>
{% endfor %}
{% for message in app.session.flashbag.get('message_global_error') %}
<div class="container">
    <br/>
    <div class="alert alert-danger" style="margin-bottom: 10px;top: 64px;">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        {{ message }}
    </div>
</div>
{% endfor %}

